{
  "name": "moo.js",
  "version": "0.4.0",
  "description": "Editor-agnostic markdown live previewer",
  "keywords": [
    "markdown",
    "preview",
    "utility",
    "generator"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/pyrocat101/moo.git"
  },
  "preferGlobal": true,
  "license": "MIT",
  "author": "Linjie Ding",
  "dependencies": {
    "express": "~3.2.4",
    "commander": "~1.1.1",
    "jade": "~0.30.0",
    "highlight.js": "~7.3.0",
    "chokidar": "~0.6.2",
    "open": "0.0.3",
    "colors": "~0.6.0-1",
    "marked": "~0.2.9",
    "lodash": "~1.2.1"
  },
  "devDependencies": {
    "coffee-script": "~1.6.2"
  },
  "main": "./moo",
  "bin": {
    "moo": "./bin/moo"
  },
  "scripts": {
    "build": "cake build",
    "build-all": "npm run build && npm run build-js && npm run build-css",
    "build-js": "cake build:js",
    "build-css": "cake build:css",
    "watch": "cake -w build"
  },
  "engines": {
    "node": ">=0.10"
  }
}
